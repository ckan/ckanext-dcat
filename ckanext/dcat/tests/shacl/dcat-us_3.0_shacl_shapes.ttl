<https://www.linkedin.com/in/stephanefellah/> a foaf:Person ;
    foaf:name "Stephane Fellah" .

dcat-us-shp:GeometryAsWKTorGMLorGeoJSON a rdf:List ;
    rdf:first [ sh:datatype gsp:wktLiteral ] ;
    rdf:rest ( [ sh:datatype gsp:gmlLiteral ] [ sh:datatype gsp:geoJSONLiteral ] ) .

dcat-us-shp:DateOrDateTimeOrYearOrYearMonth a rdf:List ;
    rdf:first [ sh:datatype xsd:date ] ;
    rdf:rest ( [ sh:datatype xsd:dateTime ] [ sh:datatype xsd:gYear ] [ sh:datatype xsd:gYearMonth ] ) .

dash:StringOrLangString a rdf:List ;
    rdf:first [ sh:datatype xsd:string ] ;
    rdf:rest ( [ sh:datatype rdf:langString ] ) .

